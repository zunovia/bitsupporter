<div class="d-flex justify-content-center mb-4">
<% if @model == 'end_user' %>
	<h3>EndUsers search for "<%= @content %>"</h3>
  <%= render 'public/end_users/index', end_users: @records %>
<% elsif @model == 'post' %>
	<h3>Posts search for "<%= @content %>"</h3>
  <%= render 'public/posts/index', posts: @records %>
<% end %>
</div>