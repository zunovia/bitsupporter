<table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% end_users.each do |end_user| %>
      <tr>
        <td><%= image_tag end_user.get_profile_image, size: '50x50' %></td>
        <td><%= end_user.name %></td>
        <td><%= render "relationships/btn", end_user: end_user %></td>
        <td><%= link_to 'Show', end_user, class: "end_user_#{end_user.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>